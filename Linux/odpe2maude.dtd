<!ELEMENT reverseinferencer EMPTY>
<!ATTLIST reverseinferencer name CDATA #REQUIRED>

<!ELEMENT sa_inferencer EMPTY>
<!ATTLIST sa_inferencer name CDATA #REQUIRED>

<!ELEMENT sa_reverseinferencer EMPTY>
<!ATTLIST sa_reverseinferencer name CDATA #REQUIRED>

<!ELEMENT normalizer EMPTY>
<!ATTLIST normalizer name CDATA #REQUIRED>

<!ELEMENT subatomiser EMPTY>
<!ATTLIST subatomiser name CDATA #REQUIRED>

<!ELEMENT interpreter EMPTY>
<!ATTLIST interpreter name CDATA #REQUIRED>

<!ELEMENT maudename EMPTY>
