<?xml version="1.0" standalone="no"?>
<!DOCTYPE system SYSTEM "description.dtd" [
    <!ENTITY % odpe2maude SYSTEM "odpe2maude.dtd">
    %odpe2maude;
]>
<system name="SKV">
  <syntax>
    <unary-prefix  symbol="-" tex="\overline{#1}"/>
    <unary-prefix  symbol="&blackstar;" input="*"/>
    <binary-outfix id="par" symbol="(⅋)" input="[,]"/>
    <binary-outfix id="tensor" symbol="(&circledTimes;)" input="(,)"/>
    <binary-outfix id="seqcon" symbol="(&leftTriangle;)" input="&lt;;&gt;" comm="no"/>
    <constant      symbol="&unfilledBullet;" input="o"/>
    <constants     symbols="a b c d e f g h i j"/>
    <constant      symbol="a&one;"   input="a1"/>
    <constant      symbol="a&two;"   input="a2"/>
    <constant      symbol="a&three;" input="a3"/>
    <constant      symbol="a&four;"  input="a4"/>
    <constant      symbol="b&one;"   input="b1"/>
    <constant      symbol="b&two;"   input="b2"/>
    <constant      symbol="b&three;" input="b3"/>
    <constant      symbol="b&four;"  input="b4"/>
    <constant      symbol="c&one;"   input="c1"/>
    <constant      symbol="c&two;"   input="c2"/>
    <constant      symbol="c&three;" input="c3"/>
    <constant      symbol="c&four;"  input="c4"/>
    <constant      symbol="d&one;"   input="d1"/>
    <constant      symbol="d&two;"   input="d2"/>
    <constant      symbol="d&three;" input="d3"/>
    <constant      symbol="d&four;"  input="d4"/>
    <constant      symbol="e&one;"   input="e1"/>
    <constant      symbol="e&two;"   input="e2"/>
    <constant      symbol="e&three;" input="e3"/>
    <constant      symbol="e&four;"  input="e4"/>
    <constant      symbol="f&one;"   input="f1"/>
    <constant      symbol="f&two;"   input="f2"/>
    <constant      symbol="f&three;" input="f3"/>
    <constant      symbol="f&four;"  input="f4"/>
    <constant      symbol="g&one;"   input="g1"/>
    <constant      symbol="g&two;"   input="g2"/>
    <constant      symbol="g&three;" input="g3"/>
    <constant      symbol="g&four;"  input="g4"/>
    <constant      symbol="e&one;"   input="h1"/>
    <constant      symbol="e&two;"   input="h2"/>
    <constant      symbol="e&three;" input="h3"/>
    <constant      symbol="e&four;"  input="h4"/>
    
     
  </syntax>
  <rules>
    <inference-rule id="identity"  name="i"/>
    <inference-rule id="cut" name="ī"/>
    <inference-rule id="switch" name="s"/> 
    <inference-rule id="seq" name="q"/>
    <inference-rule id="coseq" name="q̄"/>
    <inference-rule id="star" name="&blackstar;" />
    <inference-rule id="costar" name="&blackstaroverline;" />
    <inference-rule id="promotion" name="p"/>
    <inference-rule id="copromotion" name="p̄"/>
    <inference-rule id="contraction" name="c"/>
    <inference-rule id="cocontraction" name="c̄"/>
  </rules>
  <backend class="odpe.backend.odpe2maude.ODPE2Maude">
    <load name="nnf_SKV.maude"/>
    <load name="skv.maude"/>
    <inferencer name="SKV-Meta"/>
    <normalizer name="NNF"/>
    <maudename id="par" name="[_,_]"/>
    <maudename id="tensor" name="{_,_}"/>
    <maudename id="seqcon" name="&lt;_;_&gt;"/>
    <result-sort name="Structure"/>
  </backend>
</system>
